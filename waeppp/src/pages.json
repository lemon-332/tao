{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "tabBar": {
    "color": "#000000",
    "selectedColor": "#17adfb",
    "borderStyle": "black",
    "backgroundColor": "#ffffff",
    "list": [
      {
        "pagePath": "pages/home/index",
        "iconPath": "static/home.png",
        "selectedIconPath": "static/home-select.png",
        "text": "首页"
      },
      {
        "pagePath": "pages/god/index",
        "iconPath": "static/god.png",
        "selectedIconPath": "static/god-select.png",
        "text": "商品"
      },
      {
        "pagePath": "pages/my/index",
        "iconPath": "static/my.png",
        "selectedIconPath": "static/my-select.png",
        "text": "我的"
      }
    ]
  },
  "pages": [
    {
      "path": "pages/home/index",
      "style": {
        "navigationBarTitleText": "首页"
      }
    },
    {
      "path": "pages/god/index",
      "style": {
        "navigationBarTitleText": "购物"
      }
    },
    {
      "path": "pages/cart/index",
      "style": {
        "navigationBarTitleText": "购物车"
      }
    },
    {
      "path": "pages/my/index",
      "style": {
        "navigationBarTitleText": "我的"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  }
}
